    "react-dom": "18.2.0",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@prisma/client": "^4.5.0",
    "@types/eslint": "^8.4.7",
    "@types/node": "18.11.3",
    "@types/prettier": "^2.7.1",
