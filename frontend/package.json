{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'pages/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'pages/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write \"pages/**/*.{js,jsx,ts,tsx}\"",
    "preinstall": "typesync || :",
    "prepare": "cd .. && husky install .config/.husky"
  },
  "dependencies": {
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.7",
    "@types/node": "18.11.3",
    "@types/prettier": "^2.7.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint",
      "yarn format",
      "yarn lint:fix"
    ]
  }
}
